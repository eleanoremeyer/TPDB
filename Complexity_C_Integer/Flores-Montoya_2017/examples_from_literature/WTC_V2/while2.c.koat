(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS eval_while2_start))
(VAR v_N v_i.0 v_j.0)
(RULES
  eval_while2_start(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb0_in(v_N, v_i.0, v_j.0))
  eval_while2_bb0_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb1_in(v_N, v_N, v_j.0))
  eval_while2_bb1_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb2_in(v_N, v_i.0, v_N)) :|: v_i.0 > 0
  eval_while2_bb1_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb5_in(v_N, v_i.0, v_j.0)) :|: v_i.0 <= 0
  eval_while2_bb2_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb3_in(v_N, v_i.0, v_j.0)) :|: v_j.0 > 0
  eval_while2_bb2_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb4_in(v_N, v_i.0, v_j.0)) :|: v_j.0 <= 0
  eval_while2_bb3_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb2_in(v_N, v_i.0, v_j.0 - 1))
  eval_while2_bb4_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_bb1_in(v_N, v_i.0 - 1, v_j.0))
  eval_while2_bb5_in(v_N, v_i.0, v_j.0) -> Com_1(eval_while2_stop(v_N, v_i.0, v_j.0))
)
